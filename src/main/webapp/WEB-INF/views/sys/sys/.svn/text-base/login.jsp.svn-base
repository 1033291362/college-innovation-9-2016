<%@ page language="java" contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="/WEB-INF/views/base/sys_tag.jsp"%>
<!DOCTYPE>
<html>
<head>
	<title>登录</title>
	<link rel="stylesheet" type="text/css" href="${staticPath}/libs/xui/xui.css" />
	<link rel="stylesheet" type="text/css" href="${staticPath}/sys/css/login.css" />
	<script type="text/javascript">var basePath="${basePath}";</script>
	<script type="text/javascript">var adminPath="${adminPath}";</script>
	<script type="text/javascript" src="${staticPath}/libs/jquery/jquery-1.9.1.min.js"></script>
	<script type="text/javascript" src="${staticPath}/libs/jquery/jquery.cookie.js"></script>
	<script type="text/javascript" src="${staticPath}/libs/xui/xui.js"></script>
	<script type="text/javascript" src="${staticPath}/sys/sys/login.js"></script>
</head>
<body>
	<div class="login_box">
		<form class="xform login_panel" id="login_from">
			<div class="xgroup">
				<div class="title">登录后台</div>
			</div>
			<div class="xgroup">
				<label class="g1 name"></label>
				<div class="g2"> 
					<input type="text" name="username" placeholder="登录账号" autocomplete="off" value="admin">
				</div>
			</div>
			<div class="xgroup">
				<label class="g1 pass"></label>
				<div class="g2">
					<input type="password" name="password" autocomplete="off" value="admin">
				</div>
			</div>
			<div class="btn_box">
				<span class="xbtn" id="submit">登录</span>
			</div>
		</form>
	</div>
</body>
</html>