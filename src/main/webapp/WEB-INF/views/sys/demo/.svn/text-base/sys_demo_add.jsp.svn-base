<%@ page language="java" contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"%>
<%String basePath = request.getContextPath()+"/";%>
<div class="xwindow" id="sys_demo_win">
	<div class="cover"></div>
	<div class="xbox">
		<div class="title">crud测试表单</div>
		<form class="xform" id="sys_demo_form" method="post">			
			<input type="hidden" name="id" id="sys_demo_id"/>
			<div class="xgroup">
				<label class="g1">名称<i>*</i></label>
				<div class="g2"><input type="text" name="name"/></div>
			</div>
			<div class="xgroup">
				<label class="g1">电话<i>*</i></label>
				<div class="g2"><input type="text" name="phone"/></div>
			</div>
			<div class="xgroup">
				<label class="g1">邮箱<i>*</i></label>
				<div class="g2"><input type="text" name="email"/></div>
			</div>
			<div class="xgroup">
				<label class="g1">生日<i>*</i></label>
				<div class="g2"><input class="xinput xdate" name="birthday" id="birthday"/></div>
			</div>
		</form>
		<div class="bottombar">
			<span class="xbtn btn_submit">保存</span><span class="xbtn xclose">取消</span>
		</div>
	</div>
</div>